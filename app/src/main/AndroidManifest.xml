<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tryceo.jack.pastbroadcast">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Home"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VideoLinks"
            android:label="@string/title_activity_past_video_links"
            android:parentActivityName =".Home">
        </activity>
        <activity
            android:name=".ChunkLinks"
            android:label="@string/title_activity_video_links"
            android:parentActivityName=".VideoLinks">
        </activity>

    </application>

</manifest>
